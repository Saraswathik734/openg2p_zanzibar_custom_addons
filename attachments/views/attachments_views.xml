<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_partner_attachments_form" model="ir.ui.view">
        <field name="name">res.partner.attachments.form</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="g2p_registry_individual.view_individuals_form"/>
        <field name="arch" type="xml">

            <xpath expr="//notebook" position="inside">
                <page string="Attachments">
                    <group>
                        <group>
                            <field name="nominee_image" widget="image"/>
                        </group>
                        <group>
                            <field name="zan_image" widget="image"/>
                        </group>
                    </group>
                </page>
            </xpath>

        </field>
    </record>
</odoo>
